datasource db {
  provider             = "sqlite"
  url                  = "file:./dev.db"
  shadowDatabaseUrl    = "file:./shadow.db"
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model User {
  id Int @id @default(autoincrement())
}
