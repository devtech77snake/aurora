datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated"
  previewFeatures = ["referentialIntegrity"]
  engineType      = "library"
  binaryTargets   = ["native"]
}

model User {
  id Int @id @default(autoincrement())
}
