datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Test {
  id  Int @id @default(autoincrement())
  user User?
}
model User {
  id        Int
  firstName Int @map("fname")
  test Test @relation(fields: [firstName], references: [id])

  @@index([id])
}
