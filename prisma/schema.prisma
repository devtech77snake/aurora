model User {
	id Int @id @default(autoincrement())
	firstName String @map("first_name")
  model     Int
}

enum Test {
  TEST
}

model Person {
	id Int @id @default(autoincrement())
	firstName String @map("first_name")
  test      String  @unique @map("Test2")

  @@index([firstName, test])
  @@unique([id, test])
}