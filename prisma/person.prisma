model Person {
  createdAt  DateTime   @default(now())
  parent    User?   @relation(fields: [parentId], references: [id])
  parentId  Int @unique
  banana    String

  // @@unique(fields: [id, parentId], name: "tes")
  @@id([createdAt, parentId])
  @@index([parentId], map: "test")
}

model User {
    id Int @id @default(autoincrement())
    Person Person
    name String?
}
